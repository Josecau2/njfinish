{
  "name": "AI-CONNECT",
  "version": "8.2.3",
  "private": true,
  "scripts": {
    "build": "npm run build:frontend && npm run setup:production",
    "build:frontend": "vite build --mode production --config frontend/vite.config.mjs",
    "build:secure": "vite build --config frontend/vite.config.mjs && node frontend/scripts/security-cleaner.js",
    "build:fast": "vite build --config frontend/vite.config.dev.mjs",
    "build:analyze": "node frontend/scripts/build-analyzer.js",
    "build:backend": "npm install --production",
    "clean": "rimraf frontend/build frontend/node_modules/.vite",
    "clean:deps": "rimraf node_modules package-lock.json && npm install",
    "lint": "eslint",
    "audit:gen-manifest": "node scripts/generate-manifest.mjs",
    "audit:manifest": "npm run audit:gen-manifest && node scripts/check-manifest.mjs",
    "serve": "vite preview --config frontend/vite.config.mjs",
    "start": "node index.js",
    "dev": "node index.js",
    "dev:frontend": "vite --config frontend/vite.config.mjs",
    "dev:monitor": "node monitor-errors.js",
    "test:pricing": "node frontend/scripts/simulatePricingTest.mjs",
    "seed:dev": "node scripts/seed-dev.js",
    "db:sync": "node scripts/sync-db.js",
    "db:sync:terms": "node scripts/sync-terms.js",
    "db:wait": "node scripts/wait-for-db.js",
    "db:migrate": "node scripts/migrate.js up",
    "db:migrate:down": "node scripts/migrate.js down",
    "db:migrate:status": "node scripts/migrate.js status",
    "db:migrate:list": "node scripts/list-migrations.js",
    "db:migrate:check": "node scripts/check-migrations.js",
    "setup:production": "npm run build:backend && node check-production-setup.js && node setup-production.js && node verify-production-images.js",
    "verify:production": "node verify-production-images.js",
    "check:production": "node check-production-setup.js",
    "debug:orders:db": "node scripts/debug-list-orders.js --limit 20",
    "debug:orders:api": "node scripts/debug-fetch-orders.js",
    "test:email-quote": "npm --prefix frontend run test:pdf && node scripts/test-email-quote-e2e.js",
    "test:email-quote:full": "npm run build:frontend && npm --prefix frontend run test:pdf && node scripts/test-email-quote-e2e.js",
    "test:email-quote:to:joseca": "npm --prefix frontend run test:pdf && node scripts/test-email-quote-e2e.js --to joseca@symmetricalwolf.com",
    "send:email-quote:to:joseca": "npm --prefix frontend run test:pdf && node scripts/test-email-quote-e2e.js --to joseca@symmetricalwolf.com --send",
    "test:payments": "node --test tests/utils/payments/amounts.test.js",
    "test:audit": "playwright test",
    "test:audit:headed": "playwright test --headed",
    "test:audit:update": "playwright test --update-snapshots",
    "bundle:analyze": "vite build --config frontend/vite.config.mjs --mode analyze",
    "bundle:analyze:legacy": "npm run build:frontend && npx webpack-bundle-analyzer frontend/build/static/js/*.js",
    "seed:e2e": "node scripts/seed-e2e.js"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.27.0",
    "@chakra-ui/theme-tools": "^2.2.6",
    "@coreui/chartjs": "^4.1.0",
    "@coreui/coreui": "^5.3.1",
    "@coreui/icons": "^3.0.1",
    "@coreui/icons-react": "^2.3.0",
    "@coreui/react": "^5.5.0",
    "@coreui/react-chartjs": "^3.0.0",
    "@coreui/utils": "^2.0.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@fullcalendar/daygrid": "^6.1.18",
    "@fullcalendar/interaction": "^6.1.18",
    "@fullcalendar/list": "^6.1.18",
    "@fullcalendar/react": "^6.1.18",
    "@fullcalendar/timegrid": "^6.1.18",
    "@monaco-editor/react": "^4.7.0",
    "@popperjs/core": "^2.11.8",
    "@reduxjs/toolkit": "^2.8.2",
    "@stripe/react-stripe-js": "^4.0.2",
    "@stripe/stripe-js": "^7.9.0",
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "chokidar": "^4.0.3",
    "classnames": "^2.5.1",
    "core-js": "^3.40.0",
    "cors": "^2.8.5",
    "countries-and-timezones": "^3.8.0",
    "csv-parser": "^3.2.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^17.2.1",
    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "formik": "^2.4.6",
    "framer-motion": "^12.23.22",
    "hashids": "^2.3.0",
    "i18next": "^25.4.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.511.0",
    "multer": "^2.0.2",
    "mysql2": "^3.14.3",
    "n8n-nodes-base": "^1.17.0",
    "nodemailer": "^7.0.5",
    "pdf-parse": "^1.1.1",
    "pdfjs-dist": "^5.4.149",
    "pdfmake": "^0.2.20",
    "prop-types": "^15.8.1",
    "puppeteer-core": "^24.16.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.7.1",
    "react-pdf": "^10.1.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.8.2",
    "react-xml-viewer": "^3.0.1",
    "redux": "5.0.1",
    "sanitize-html": "^2.17.0",
    "sequelize": "^6.37.7",
    "stripe": "^18.5.0",
    "umzug": "^3.8.0",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@playwright/test": "^1.55.1",
    "@types/node": "^24.6.2",
    "@vitejs/plugin-react": "^5.0.2",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "fast-glob": "^3.3.3",
    "form-data": "^4.0.4",
    "glob": "^11.0.3",
    "globals": "^15.15.0",
    "node-fetch": "^3.3.2",
    "postcss": "^8.5.2",
    "prettier": "3.5.1",
    "rimraf": "^6.0.1",
    "rollup-plugin-visualizer": "^6.0.3",
    "sass": "^1.85.0",
    "terser": "^5.43.1",
    "vite": "^7.1.3",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
